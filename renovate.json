{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchFileNames": ["misskey/deployment.yaml"],
      "matchDatasources": ["docker"],
      "matchPackageNames": ["misskey/misskey"],
      "versioning": "regex:^(?<major>\\d{4})(\\.(?<minor>\\d+))(\\.(?<patch>\\d+))$",
      "automerge": false
    }
  ]
}
