{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["misskey/misskey"],
      "versioning": "regix:^(?<major>\\d\\4*)(\\.(?<minor>\\d+))(\\.(?<patch>\\d+))$",
      "automerge": false
    }
  ]
}
